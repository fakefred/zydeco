data LList' (A : VType) where
  | Nil()
  | Cons(A, U(F(LList' A)))
end

let rec iota : Int -> F(LList' Int) = (
  fn (i: Int) ->
    do i+1 <- ! add i 1;
    ret Cons(i, { ! iota i+1 })
);

let rec foldr : U(F(LList' Int)) -> U(Int -> U(OS) -> OS) -> U(OS) -> OS = (
  fn ll f b ->
    do llv <- ! ll;
    match llv
    | Nil() -> ! b
    | Cons(a, ll) -> ! f a { ! foldr ll f b }
    end
);

let loopy = {
  ! foldr
  { ! iota 0 }
  { fn (x: Int) (b: U(OS)) ->
    do next? <- ! int_lt x 10;
    ! write_int_line x b
  }
  { ! panic "unreachable" }
};

! loopy